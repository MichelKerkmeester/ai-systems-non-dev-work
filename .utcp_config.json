{
  "load_variables_from": [
    {
      "variable_loader_type": "dotenv",
      "env_file_path": ".env"
    }
  ],
  "tool_repository": {
    "tool_repository_type": "in_memory"
  },
  "tool_search_strategy": {
    "tool_search_strategy_type": "tag_and_description_word_match"
  },
  "manual_call_templates": [
    {
      "name": "chrome_devtools_1",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "chrome_devtools_1": {
            "transport": "stdio",
            "command": "npx",
            "args": [
              "chrome-devtools-mcp@latest",
              "--isolated=true"
            ],
            "env": {}
          }
        }
      }
    },
    {
      "name": "chrome_devtools_2",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "chrome_devtools_2": {
            "transport": "stdio",
            "command": "npx",
            "args": [
              "chrome-devtools-mcp@latest",
              "--isolated=true"
            ],
            "env": {}
          }
        }
      }
    },
    {
      "name": "clickup",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "clickup": {
            "transport": "stdio",
            "command": "npx",
            "args": [
              "-y",
              "@taazkareem/clickup-mcp-server@latest"
            ],
            "env": {
              "CLICKUP_API_KEY": "${CLICKUP_API_KEY}",
              "CLICKUP_TEAM_ID": "${CLICKUP_TEAM_ID}"
            },
            "_note": "ClickUp MCP server for task management. Get API token from ClickUp Settings > Apps > API Token."
          }
        }
      }
    },
    {
      "name": "figma",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "figma": {
            "transport": "stdio",
            "command": "npx",
            "args": [
              "-y",
              "figma-developer-mcp",
              "--stdio"
            ],
            "env": {
              "FIGMA_API_KEY": "${FIGMA_API_KEY}"
            },
            "_note": "Framelink Figma MCP. Uses variable substitution from .env via load_variables_from."
          }
        }
      }
    },
    {
      "name": "github",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "github": {
            "transport": "stdio",
            "command": "npx",
            "args": [
              "-y",
              "@modelcontextprotocol/server-github"
            ],
            "env": {
              "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
            }
          }
        }
      }
    },
    {
      "name": "imagician",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "imagician": {
            "transport": "stdio",
            "command": "docker",
            "args": [
              "exec",
              "-i",
              "imagician",
              "node",
              "/app/dist/index.js"
            ],
            "env": {},
            "_note": "Image editing MCP server via Docker (resize, convert, crop, compress, rotate, flip). Source: https://github.com/flowy11/imagician"
          }
        }
      }
    },
    {
      "name": "jianying",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "jianying": {
            "transport": "stdio",
            "command": "docker",
            "args": [
              "exec",
              "-i",
              "jianying-mcp",
              "uv",
              "run",
              "python",
              "-m",
              "jianyingdraft.server"
            ],
            "env": {},
            "_note": "JianYing (CapCut) video editing MCP server (Docker). 18 tools for draft creation, video/audio/text segments, animations, transitions, effects. Build: cd 'CapCut/mcp server/jianying-mcp' && docker-compose up -d --build"
          }
        }
      }
    },
    {
      "name": "notion",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "notion": {
            "transport": "stdio",
            "command": "docker",
            "args": [
              "exec",
              "-i",
              "notion-mcp",
              "notion-mcp-server"
            ],
            "env": {
              "NOTION_TOKEN": "${NOTION_TOKEN}"
            },
            "_note": "Notion MCP server (Docker). Official Notion integration for pages, databases, search. Build: cd 'Notion/mcp server/notion-mcp' && docker-compose up -d --build"
          }
        }
      }
    },
    {
      "name": "video-audio",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "video-audio": {
            "transport": "stdio",
            "command": "docker",
            "args": [
              "exec",
              "-i",
              "video-audio",
              "python",
              "/app/server.py"
            ],
            "env": {},
            "_note": "FFmpeg-powered video/audio editing MCP server via Docker. Source: https://github.com/misbahsy/video-audio-mcp"
          }
        }
      }
    },
    {
      "name": "webflow",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "webflow": {
            "transport": "stdio",
            "command": "npx",
            "args": [
              "mcp-remote",
              "https://mcp.webflow.com/sse"
            ],
            "env": {}
          }
        }
      }
    }
  ]
}
